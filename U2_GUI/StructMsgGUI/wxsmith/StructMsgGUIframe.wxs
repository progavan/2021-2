<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="StructMsgGUIFrame">
		<title>Emails list accumulator</title>
		<id_arg>0</id_arg>
		<object class="wxTextCtrl" name="ID_TEXTCTRL_NOTEPAD" variable="TextCtrlNotePad" member="yes">
			<pos>128,144</pos>
			<style>wxTE_MULTILINE</style>
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idFileOpen" variable="MenuItem3" member="yes">
					<label>&amp;Open</label>
					<accel>Ctrl+O</accel>
					<help>Abre un archivo de texto.</help>
					<handler function="OnFileOpen" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_MENUITEM1" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
		<object class="wxFileDialog" variable="NotePadFileDialog" member="yes">
			<message>Select file</message>
			<wildcard>*.txt</wildcard>
		</object>
	</object>
</wxsmith>
